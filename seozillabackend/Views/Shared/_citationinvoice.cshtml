@model seozillabackend.Models.order

<div class="row">
    <div class="table-responsive">
        <table class="table">
            <thead>
                <tr>
                <tr>
                    <th>Quantity</th>
                    <th>Details</th>
                    <th>Unit Cost</th>
                    <th>Subtotal</th>
                    <th></th>


                </tr>
            </thead>
            <tbody>
                @foreach (var itemcitation in Model.citations.Take(1))
                {
                    <tr>

                        <td class="count">@ViewBag.countcitation</td>
                        <td class="details">@itemcitation.plan @ViewBag.service</td>
                        <td>£<span class="unit-cost"> </span></td>
                        <td>£<span class="total-cost"> </span></td>
                    </tr>
                }
            </tbody>

        </table>

    </div>
</div>

