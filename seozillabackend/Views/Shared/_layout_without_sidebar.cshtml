    <script>
        @*Scripts*@

        $(document).ready(function () {

          
          
            $(".package-qty").on("change", function () {


                var $quantity = $(this);


                id = $quantity.data("package-id");

                quantity = parseInt($quantity.val());

                id1 = $("#package-data-" + id).index();


               




                if (isNaN(quantity) || quantity < 0 || quantity == 0)
                {

                    $quantity.val(0);

                    quantity = 0;

                    $("#package-data-" + id).addClass("hidden");

                    $("#package-data-" + id).nextAll(".form-group-row").remove();

                }

                if (quantity > 0) {

                    $("#package-data-" + id).removeClass("hidden");

                    $("#package-data-" + id).find(".form-group-row").remove();



                    for (var i = 0; i < quantity; i++)
                    {


                        $("#package-data-" + id + " .panel-sub-heading").after(pack);

                        if ($("#blog_order").attr("name") == undefined)
                            $("#blog_order").attr("name", "[" + i + "].daordered");

                        if ($("#select1").attr("name") == undefined)
                            $("#select1").attr("name", "[" + i + "].wordcount");

                        if ($("#text1").attr("name") == undefined)
                            $("#text1").attr("name", "[" + i + "].anchortext");


                        if ($("#text2").attr("name") == undefined)
                            $("#text2").attr("name", "[" + i + "].targeturl");


                        if ($("#select2").attr("name") == undefined)
                            $("#select2").attr("name", "[" + i + "].country");

                        if ($("#text3").attr("name") == undefined)
                            $("#text3").attr("name", "[" + i + "].businessname");

                        if ($("#text4").attr("name") == undefined)
                            $("#text4").attr("name", "[" + i + "].websiteurl");

                        if ($("#text5").attr("name") == undefined)
                            $("#text5").attr("name", "[" + i + "].businessdescription");

                        if ($("#text6").attr("name") == undefined)
                            $("#text6").attr("name", "[" + i + "].keywords");

                        if ($("#text7").attr("name") == undefined)
                            $("#text7").attr("name", "[" + i + "].founder");

                        if ($("#text8").attr("name") == undefined)
                            $("#text8").attr("name", "[" + i + "].address");

                        if ($("#text9").attr("name") == undefined)
                            $("#text9").attr("name", "[" + i + "].phone");

                        if ($("#text10").attr("name") == undefined)
                            $("#text10").attr("name", "[" + i + "].email");

                        if ($("#blog_order").attr("value") == undefined)

                            if (id1 == 0)
                            {

                                $("#blog_order").attr("value", "ten_plus");
                            }
                            else if (id1 == 1)
                            {

                                $("#blog_order").attr("value", "twenty_plus");
                            }
                            else if (id1 == 2)
                            {

                                $("#blog_order").attr("value", "thirty_plus");
                            }
                            else if (id1 == 3)
                            {

                                $("#blog_order").attr("value", "forty_plus");
                            }
                            else if (id1 == 4)
                            {

                                $("#blog_order").attr("value", " ten_plus_special");
                            }

                      

                    }
                }
                
                var Cost = $("#oldCost-" + id).text();
                var newcost = (Cost * quantity);

                
                    $("#newCost-" + id).text(newcost); 

                    var new1 = parseInt($("#newCost-1").text());
                    var new2 =  parseInt($("#newCost-2").text());
                    var new3 = parseInt ($("#newCost-3").text());
                    var new4 =  parseInt($("#newCost-4").text());
                                    
                    $(".totalcost").text(new1 + new2 + new3 + new4);

                    $(".totalcost_usd").text(Math.round((new1 + new2 + new3 + new4) * (1.40)));

                    if (quantity == 5) {
                         $(".totalcost").text((new1 + new2 + new3 + new4) - (25));

                        $(".totalcost_usd").text(Math.round((new1 + new2 + new3 + new4) * (1.40)) - (100));


                    }
                    if (quantity == 10) {
                        $(".totalcost").text(new1 + new2 + new3 + new4);

                        $(".totalcost_usd").text(Math.round((new1 + new2 + new3 + new4) * (1.40)));

                    }

                    if(quantity<5 && quantity!=0)
                    {
                        $(".approx_days").html('Approx 14 Days');
                    }
                   if (quantity >5 )
                    {
                        $(".approx_days").html('Approx 21 Days');
                   }
                if (quantity > 25 )
                    {
                    $(".approx_days").html('  Approx 28 Days');
                   }
                 
                    if (quantity == 0) {
                        $(".approx_days").html(NaN);
                    }

                   

                        
                        
                        if(id==1 && quantity==1)
                        {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da10%2B-1pack");
                 
                        }
                        if (id == 1 && quantity == 5)
                        {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da10%2B-5packs");
                 
                        }
                        if (id == 1 && quantity == 10) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da10%2B-10packs");

                        }
                        if (id == 2 && quantity == 1) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-20%2B-1pack");

                        }
                        if (id == 2 && quantity == 5) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-20%2B-5packs");

                        }
                        if (id == 2 && quantity == 10) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-20%2B-10packs");

                        }

                        if (id == 3 && quantity == 1) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da30%2B-1packs");

                        }
                        if (id == 3 && quantity == 5) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/-blogs-da30%2B5packs");

                        }
                        if (id == 3 && quantity == 10) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da30%2B-10packs");

                        }
                        if (id == 4 && quantity == 1) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da40%2B-1pack");

                        }
                        if (id == 4 && quantity == 5) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da40%2B-5packs");

                        }
                        if (id == 4 && quantity == 10) {
                            $("#url").attr("value", "https://amit-test.chargebee.com/hosted_pages/plans/blogs-da40%2B-10packs");

                        }
                       
                    
 

                    $('input[name="radio-button"]').click(function () {

                        if ($('input[name="radio-button"]').is(':checked')) {

                            var radioValue = $("input[name='radio-button']:checked").val();

                            switch(radioValue)
                            {
                                case "radio-1":
                                    $("#select1").prop("disabled", false);
                                    $("#select2").prop("disabled", true);
                                    $("#select3").prop("disabled", true);
                                    $("#select4").prop("disabled", true);
                                    break;

                                case "radio-2":
                                    $("#select2").prop("disabled", false);
                                    $("#select1").prop("disabled", true);
                                    $("#select3").prop("disabled", true);
                                    $("#select4").prop("disabled", true);

                                    break;

                                case "radio-3":
                                    $("#select3").prop("disabled", false);
                                    $("#select1").prop("disabled", true);
                                    $("#select2").prop("disabled", true);
                                    $("#select4").prop("disabled", true);

                                    break;

                                case "radio-4":
                                    $("#select4").prop("disabled", false);
                                    $("#select1").prop("disabled", true);
                                    $("#select2").prop("disabled", true);
                                    $("#select3").prop("disabled", true);
                                    break;

                                default:
                                    $("#select4").prop("disabled", true);
                                    $("#select1").prop("disabled", true);
                                    $("#select2").prop("disabled", true);
                                    $("#select3").prop("disabled", true);


                            }

                            $("#package-data-" + id).addClass("hidden");

                            $("#package-data-" + id).find(".form-group-row").remove();

                            $quantity.val(0);

                            $("#newCost-" + id).text(0);

                            $(".totalcost").text(0);

                            $(".totalcost_usd").text(0);


                           
                        }

                    });


            });

           
    });
    </script>



