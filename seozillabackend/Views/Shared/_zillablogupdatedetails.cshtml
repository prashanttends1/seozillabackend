@model seozillabackend.Models.zillablog


<div class="form-horizontal">

    <div class="form-group hidden">

        @Html.LabelFor(model => model.words, htmlAttributes: new { @class = "control-label col-md-2 " })
        <div class="input-group">
            <div class="input-group-addon"><i class="fa fa-briefcase"></i></div>
            @Html.TextBoxFor(model => model.words, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.words)
        </div>
    </div>



    <div class="form-group hidden">
        @Html.LabelFor(model => model.quantity, htmlAttributes: new { @class = "control-label col-md-2 " })
