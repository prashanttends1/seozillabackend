@model IEnumerable<seozillabackend.Models.zillalinkbuilding>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_layout_with_sidebar.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.url1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.url2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.url3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.keyword1a)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.keyword1b)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.keyword2a)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.keyword2b)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.keyword3a)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.keyword3b)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cloudurl)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.url1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.url2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.url3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.keyword1a)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.keyword1b)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.keyword2a)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.keyword2b)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.keyword3a)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.keyword3b)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cloudurl)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
